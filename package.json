{
  "name": "covid-management-backend",
  "version": "1.0.0",
  "description": "final year capstone",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --collect-coverage --testTimeout=10000 --detectOpenHandles --runInBand --forceExit",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "server": "nodemon -r dotenv/config index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sparshsamir1993/covid-management-backend.git"
  },
  "keywords": [
    "covid",
    "management"
  ],
  "author": "Sparsh Samir",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sparshsamir1993/covid-management-backend/issues"
  },
  "homepage": "https://github.com/sparshsamir1993/covid-management-backend#readme",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mung": "^0.5.1",
    "geolib": "^3.3.1",
    "jest": "^26.3.0",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^3.0.2",
    "sequelize": "^5.21.12",
    "sequelize-cli": "^5.5.1",
    "sequelize-mock": "^0.10.2",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "redis-mock": "^0.51.0"
  }
}
